{
    "family": "test-frontend",
    "executionRoleArn": "arn:aws:iam::690556989199:role/ecsTaskExecutionRole",
    "taskRoleArn": "arn:aws:iam::690556989199:role/ecsTaskExecutionRole",
    "networkMode": "awsvpc", 
    "containerDefinitions": [
        {
            "name": "test-frontend",
            "image": "690556989199.dkr.ecr.ap-southeast-1.amazonaws.com/test-frontend:latest",
            "portMappings": [
                {
                    "containerPort": 80,
                    "hostPort": 80, 
                    "protocol": "tcp"
                }
            ],
            "logConfiguration": {
                "logDriver": "awslogs",
                "options": {
                    "awslogs-group": "test-frontend",
                    "awslogs-region": "ap-southeast-1",
                    "awslogs-stream-prefix": "test-frontend"
                }
            },
            "essential": true
        }
    ], 
    "cpu": "256",
    "memory": "512",
    "requiresCompatibilities": [
        "FARGATE"
    ]
}
